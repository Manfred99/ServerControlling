<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ChooseActicity">

    <ImageButton
        android:id="@+id/imageButtonProc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="72dp"
        android:layout_marginEnd="166dp"
        android:layout_marginLeft="166dp"
        android:layout_marginRight="166dp"
        android:layout_marginStart="166dp"
        android:layout_marginTop="96dp"
        android:contentDescription="@string/imageButtonProc"
        app:layout_constraintBottom_toTopOf="@+id/imageButtonFiles"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/process" />

    <ImageButton
        android:id="@+id/imageButtonFiles"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="300dp"
        android:layout_marginEnd="166dp"
        android:layout_marginLeft="166dp"
        android:layout_marginRight="166dp"
        android:layout_marginStart="166dp"
        android:layout_marginTop="72dp"
        android:contentDescription="@string/imageButtonFiles"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonProc"
        app:srcCompat="@mipmap/files" />
</android.support.constraint.ConstraintLayout>